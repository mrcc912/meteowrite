<script type="text/javascript">
(function(randid) {

Sacbee.load([ 'Shortener', 'API' ], function() {
	      Sacbee.Shortener.shorten(window.location.href.replace('//preview.', '//www.'), function(evt) {
	      									  	     		   var lurl = evt.longURL;
													       	      var url = (evt.success) ? evt.shortURL : lurl;
														      	      	/*
																   var lurl = window.location.href;
																       	      var url = lurl;
																	      	      	*/
																			   var act = new gigya.socialize.UserAction();
																			       	     
																				        
																					              
																						               
																							           act.setTitle('Sacbee - ' + unescape("Elk%20Grove%20eyes%20%24100%20million%20stadium%20as%20economic%20catalyst"));
																								   			      act.setDescription(unescape("Elk%20Grove%20is%20shooting%20for%20a%20major%20league%20goal%20of%20building%20a%20%24100%20million%20soccer%20stadium%20filled%20with%2018%2C000%20screaming%20fans."));
																											          
																												       act.addMediaItem({ type: 'image', src: 'http://media.sacbee.com/smedia/2013/03/03/19/30/mcI7O.Em.4.jpeg', href: url });
																												       			  		      
																																	          act.addActionLink('Read this story', url);
																																		  			       	       

																																						          act.setLinkBack(url);

																																							     showShareBarUI_params = {
																																							     			        containerID: randid,
																																												         shortURLs: (evt.success) ? 'never' : 'whenRequired',
																																													 	    		    	      shareButtons: [ ],
																																																	      		         showCounts: 'right',
																																																				 	         userAction: act
																																																						 	        };

																																																								   
																																																								       
																																																								            showShareBarUI_params.shareButtons.push({ provider: 'Share'/*, iconImgUp: 'https://cdns.gigya.com/gs/i/sharebar/icons/share3.png' */, enableCount: false });
																																																									    					      			   	      
																																																																		         
																																																																			     
																																																																			          showShareBarUI_params.shareButtons.push({ provider: 'Facebook', enableCount: false });
																																																																				  					    	      		  	       
																																																																													          
																																																																														      
																																																																														           showShareBarUI_params.shareButtons.push({ provider: 'Twitter', enableCount: false });
																																																																															   					     	       		  	       
																																																																																								          
																																																																																									      
																																																																																									           showShareBarUI_params.shareButtons.push({ provider: 'googleplus-share', enableCount: false });
																																																																																										   					     	       			   		
																																																																																																					   
																																																																																																					       
																																																																																																					            showShareBarUI_params.shareButtons.push({ provider: 'Stumbleupon', 'url': lurl, enableCount: false });
																																																																																																						    					      			       	      
																																																																																																															         
																																																																																																																     
																																																																																																																          showShareBarUI_params.shareButtons.push({ provider: 'Email' });
																																																																																																																	  					    	      	       
																																																																																																																								          

																																																																																																																									     gigya.socialize.showShareBarUI(showShareBarUI_params);
																																																																																																																									       }, 'sb-bitly', { }); //shorten()
																																																																																																																									       }); //load([ 'Shortener', 'API' ])

})(gigyaShare_randid);
</script>

 

 
 
  








